int token(Lexbuf);
int scan_charconst(Lexbuf);
int strng(Lexbuf);
int strng_next(Lexbuf);
int wstrng(Lexbuf);
int wstrng_next(Lexbuf);
int comment(Lexbuf);
//int snarf_asm(Lexbuf);
//int snarf_asm_body(Lexbuf);
