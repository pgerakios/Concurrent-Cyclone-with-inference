static char cnst2char(cnst_t cnst, Lexbuf lb) {
  switch (cnst) {
  case {.Int_c      = $(_,i)}: return (char)i;
  case {.LongLong_c = $(_,i)}: return (char)i;
  case {.Char_c     = $(_,c)}: return c;
  default: err("couldn't convert constant to char!",lb); return (char)0;
  }
}

